---
group: Examples
title: Example prototype
layout: example/views/Author8col
---
{% from "components/button/_macro.njk" import onsButton %}

{% set form =  {
    "method": "GET",
    "attributes": {
        "name": "myForm",
        "onsubmit":  "return validateForm()"
    }
} %}

{% block head %}
<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />

<style>
.material-symbols-rounded {
  font-variation-settings:
  'FILL' 1,
  'wght' 400,
  'GRAD' 0,
  'opsz' 48
}

.material-symbols-rounded{
    color: black;
    background-color: #FAC064;
}
</style>
{% endblock %}

{% block preMain %}
{{
    onsBreadcrumbs({
        "ariaLabel": 'Back',
        "itemsList": [
            {
                "url": '/prototypes/example/login_journeys/',
                "id": "back",
                "text": 'Back',
                "attributes": {
                    "data-attribute": "Example attribute"
                }
            }
        ]
    })
}}
{% endblock %}

{% block main %}
    <h1 class="ons-u-mb-s"><span class="ons-u-fs-m ons-u-db ons-u-mb-xs ons-u-lighter">Create an account</span>Enter your details</h1>

    {{
        onsInput({
            "id": "fullname",
            "name": "fullname",
            "label": {
                "text": "Full name"
            }

        })
    }}

    {{
        onsInput({
            "id": "email",
            "name": "email",
            "label": {
                "text": "Email address"
            }
        })
    }}

    {{
        onsInput({
            "id": "Confirmemail",
            "name": "Confirmemail",
            "label": {
                "text": "Confirm email address"
            }
        })
    }}


    {{
        onsButton({
            "text": "Continue",
            "classes": "ons-u-mt-l ons-u-mb-l"
        })
    }}

    <p>If you already have an account, you can <a href="/prototypes/example/login_journeys/">sign in</a></p>
    


{% endblock %}

{% block bodyEnd %}
    <script>

    </script>
{% endblock %}
